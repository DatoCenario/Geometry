var cube = new Cube(100, new Vector3(0,0,0));
cube.Rotate(angle, axisType);
cube.Move(angle, axisType);
cube.Scale(coef);
cube.AddCollider(ICollider);
cube.AddTexture(ITexture);
cube.AddShader(IShader);


cube/thor/polygon/conus/pyramid 


var scene = new Scene();
scene.AddPrimitive(cube);

var light = new DirectionalLight(vector3:pos, vector3:lookat);
scene.AddLight(light);


IRender;
var render = new Render(ICamera:camera);
render.RenderSceneAsync(scene);